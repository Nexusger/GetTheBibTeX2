<div class="panel-heading AuthorKnockoutTable">Autor: <!--ko text: model.Title--><!--/ko--></div>
<div class="panel-body AuthorKnockoutTable">
    <table class="table table-striped table-condensed">
        <thead>
            <tr>
                <th></th>
                <th>Titel</th>
            </tr>
        </thead>
        <tbody data-bind="foreach: authorModel.Publications">
            <tr>
                <td><input type="checkbox" data-bind="click:toggleCheckbox, checked:selected" /></td>
                <td colspan="2" data-bind="text: Title">Der Titel</td>
            </tr>
        </tbody>
    </table>
</div>
